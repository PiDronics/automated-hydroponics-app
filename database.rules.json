{
  "rules": {
    "users":{
      "$uid":{
        "systemData":{
          "$device":{
            "sensorData":{
              "$sensor":{
                "allData":{
                  ".indexOn": "time"
                }
              }
            }
          }
        },
        ".read": "$uid === auth.uid"
      }
    }
  }
}
